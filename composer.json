{
    "name": "seeren/router",
    "description": "Route http request to controller",
    "keywords": [
	    "route",
	    "http",
	    "request"
	],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Cyril Ichti",
        "email": "consultant@seeren.fr",
        "homepage" : "http://www.seeren.fr"
      }
    ],
    "minimum-stability": "dev",
    "require": {
      "php" : ">=7.2.0",
      "seeren/container": "dev-master",
      "seeren/controller": "dev-master",
      "seeren/http": "dev-master"
    },
    "autoload": {
      "psr-4": {
		"Seeren\\Router\\": "src/"
      }
    },
	"autoload-dev" : {
		"psr-4" : {
			"Seeren\\Router\\Test\\" : "test/"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "7.0.2",
		"satooshi/php-coveralls" : "2.0.0"
	}
}
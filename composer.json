{
  "name": "seeren/router",
  "description": "Route http requests to controller actions",
  "keywords": [
    "route",
    "controller",
    "http",
    "request"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cyril Ichti",
      "email": "consultant@seeren.fr",
      "homepage": "https://github.com/seeren/router"
    }
  ],
  "require": {
    "php": ">=7.4",
    "seeren/container": "~2.0",
    "seeren/controller": "~4.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Seeren\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seeren\\Router\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.4.0",
    "php-coveralls/php-coveralls": "v2.4.1"
  }
}